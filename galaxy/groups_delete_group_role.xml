<?xml version="1.0"?>
<tool id="parsec.groups.delete_group_role" name="groups" version="1.0">
 <description>delete_group_role</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <command detect_errors="aggressive">
parsec groups delete_group_role
--group_id "$group_id"
--role_id "$role_id"


| jq -S . > $results
</command>
  <inputs>
    <!-- arguments -->
    <input name="group_id" label="group_id" argument="group_id"  type="text" help="Encoded group ID"/>
    <input name="role_id" label="role_id" argument="role_id"  type="text" help="Encoded role ID to remove from the group"/>

    <!-- options -->

  </inputs>
  <outputs>
    <data format="json" name="results"/>
  </outputs>
  <help>
Remove a role from the given group.
  </help>
</tool>
