<?xml version="1.0"?>
<tool id="parsec.histories.delete_dataset_collection" name="histories" version="1.0">
 <description>delete_dataset_collection</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <command detect_errors="aggressive">
parsec histories delete_dataset_collection
--history_id "$history_id"
--dataset_collection_id "$dataset_collection_id"


| jq -S . > $results
</command>
  <inputs>
    <!-- arguments -->
    <input name="history_id" label="history_id" argument="history_id"  type="text" help="Encoded history ID"/>
    <input name="dataset_collection_id" label="dataset_collection_id" argument="dataset_collection_id"  type="text" help="Encoded dataset collection ID"/>

    <!-- options -->

  </inputs>
  <outputs>
    <data format="json" name="results"/>
  </outputs>
  <help>
Mark corresponding dataset collection as deleted.
  </help>
</tool>
