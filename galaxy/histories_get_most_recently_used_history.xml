<?xml version="1.0"?>
<tool id="parsec.histories.get_most_recently_used_history" name="histories" version="1.0">
 <description>get_most_recently_used_history</description>
	<macros>
		<import>macros.xml</import>
	</macros>
	<expand macro="requirements"/>
	<command detect_errors="aggressive">
parsec histories get_most_recently_used_history


| jq -S . > $results
</command>
