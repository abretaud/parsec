<?xml version="1.0"?>
<tool id="parsec.histories.get_status" name="histories" version="1.0">
 <description>get_status</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <command detect_errors="aggressive">
parsec histories get_status
--history_id "$history_id"


| jq -S . > $results
</command>
  <inputs>
    <!-- arguments -->
    <param name="history_id" label="history_id" argument="history_id"  type="text" help="Encoded history ID"/>

    <!-- options -->

  </inputs>
  <outputs>
    <data format="json" name="results"/>
  </outputs>
  <help>
Returns the state of this history
  </help>
</tool>
