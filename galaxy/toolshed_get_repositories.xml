<?xml version="1.0"?>
<tool id="parsec.toolshed.get_repositories" name="toolshed" version="1.0">
 <description>get_repositories</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <command detect_errors="aggressive">
parsec toolshed get_repositories


| jq -S . > $results
</command>
  <inputs>
    <!-- arguments -->

    <!-- options -->

  </inputs>
  <outputs>
    <data format="json" name="results"/>
  </outputs>
  <help>
Get the list of all installed Tool Shed repositories on this Galaxy instance.
  </help>
</tool>
